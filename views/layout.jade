include mixin/ko.jade

doctype html
html
  head
    title= title
    link(rel="stylesheet", type="text/css", href="//cdn.staticfile.org/twitter-bootstrap/3.3.1/css/bootstrap.min.css")
    block css
  body
    block content
    block js
      script(type="text/javascript", src="//cdn.staticfile.org/d3/3.4.13/d3.js")
      script(type="text/javascript", src="//cdn.staticfile.org/require.js/2.1.15/require.js")
      script.
        require.config({
          baseUrl: './javascripts',
          paths :{
            'ko' : '//cdn.staticfile.org/knockout/3.4.0/knockout-min',
            'json' : './libs/require-json',
            '_' : '//cdn.staticfile.org/underscore.js/1.7.0/underscore'
          }
        });
      script(type="text/javascript", src="//cdn.staticfile.org/jquery/1.11.1/jquery.js")
      script(type="text/javascript", src="//cdn.staticfile.org/twitter-bootstrap/3.3.1/js/bootstrap.js")